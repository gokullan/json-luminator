{
  "manifest_version": 2,
  "name": "Weblighter",
  "version": "1.0",
  "description": "A persistent web hightligher!",
  "icons": {
    "48": "icons/highlighter-solid.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["lodash.min.js", "messages.js", "weblighter.js"]
    }
  ],
  "user_scripts": {
  },
  "permissions": [
  "*://*.mozilla.org/*"
]
}
