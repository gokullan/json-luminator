{
  "manifest_version": 2,
  "name": "Weblighter",
  "version": "1.0",
  "description": "A persistent web hightligher!",
  "icons": {
    "48": "icons/highlighter-solid.svg"
  },
  "content_scripts": [
    {
      "matches": ["https://*.org/*"],
      "js": [
        "external/lodash.js",
        "core/constants.js", "core/helper.js", "core/highlight-dehighlight.js", "core/store-restore.js",
        "weblighter.js", "saved.js",
        "tests/test.js"
      ]
    }
  ],
  "user_scripts": {
  },
  "permissions": [
  "*://*.mozilla.org/*"
]
}
